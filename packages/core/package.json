{
  "name": "@postgrestx/core",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "postgrestx-generate": "./dist/bin/generate-types.cjs"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "test": "vitest run",
    "generate:types": "node ./dist/bin/generate-types.js"
  },
  "peerDependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^22.17.2",
    "eslint": "^9.33.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4"
  }
}
